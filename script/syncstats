curl -o data/defense.js     -# 'http://stats.nba.com/js/data/sportvu/defenseData.js'
curl -o data/rebounding.js  -# 'http://stats.nba.com/js/data/sportvu/reboundingData.js'
curl -o data/drives.js      -# 'http://stats.nba.com/js/data/sportvu/drivesData.js'
curl -o data/catch-shoot.js -# 'http://stats.nba.com/js/data/sportvu/catchShootData.js'
